<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="554.0" prefWidth="434.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.databasefx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button fx:id="dropButton" onAction="#onDropButtonClick" text="Drop Table" />
   <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreateTableButtonClick" text="Create Table" />
   <TextField fx:id="idField" text="id" />
   <TextField fx:id="txtName" text="name" />
   <Button fx:id="addbutton" mnemonicParsing="false" onAction="#onaddbuttonclick" text="Add Data" />
   <TableView fx:id="s_id_column" prefHeight="114.0" prefWidth="394.0">
     <columns>
       <TableColumn fx:id="s_id_coloum" prefWidth="75.0" text="id" />
       <TableColumn fx:id="s_name_colomn" prefWidth="75.0" text="name" />
     </columns>
   </TableView>
   <Button fx:id="deletebutton" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
   <Button fx:id="Searchbutton" mnemonicParsing="false" onAction="#onSearchButtonClick" text="Search All" />
</VBox>
